<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:Mordorings.ViewModels"
                    xmlns:controls="clr-namespace:Mordorings.Controls"
                    xmlns:dungeonState="clr-namespace:Mordorings.Modules.DungeonState">

    <DataTemplate DataType="{x:Type TypeName=viewModels:EditMenuViewModel}">
        <controls:EditMenu />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type TypeName=viewModels:MapMenuViewModel}">
        <controls:MapMenu />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type TypeName=viewModels:CalculationsMenuViewModel}">
        <controls:CalculationsMenu />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type TypeName=viewModels:SimulationsMenuViewModel}">
        <controls:SimulationsMenu />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type TypeName=dungeonState:DungeonStateViewModel}">
        <dungeonState:DungeonStateControl />
    </DataTemplate>
    
</ResourceDictionary>
